{
  "name": "semantique",
  "version": "1.5.0",
  "description": "Automated semantic versioning.",
  "main": "lib",
  "bin": "./lib",
  "scripts": {
    "start": "node lib",
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steve-gray/semantique.git"
  },
  "keywords": [
    "semver",
    "semantic",
    "versioning",
    "automatic"
  ],
  "author": "Steve Gray",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/steve-gray/semantique/issues"
  },
  "homepage": "https://github.com/steve-gray/semantique#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "config": "^1.24.0",
    "defaults-deep": "^0.2.3",
    "semver": "^5.3.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-mocha": "^3.0.1",
    "mocha": "^3.1.2",
    "nyc": "^8.4.0"
  },
  "eslintConfig": {
    "extends": "airbnb"
  },
  "nyc": {
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "include": [
      "lib/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "all": true,
    "sourceMap": true,
    "instrument": true,
    "check-coverage": true
  }
}
