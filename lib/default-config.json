    "exitCodes": {
        "dirtyWorkspace": -2,
        "error": -1
    },
    "commands": {
        "fetchTags": "git fetch --tags",
        "log": "git log --date=iso --pretty=format:\"%H ||| %an ||| %s ||| %d\"",
        "logDelimiter": "|||",
        "outputTerminator": "\n",
        "pending": "git diff --name-only",
        "push": "git push origin master --tags"
    },
    "defaultVersion": "1.0.0",
    "commitMessage": "[CI SKIP] Updated to version %s for release.",
    "requireCleanWorkspace": true,
    "applyNpmVersion": true,
    "pushUpstream": true,
    "outputBreak":  "===========================================================================================",
    "defaultType": {
        "type": "none",
        "color": "white",
        "background": "bgWhite",
        "priority": -1
    },
    "types": [
        {
            "type": "major",
            "color": "red",
            "background": "bgRed",
            "prefix": "break",
            "priority": 3
        },
        {
            "type": "minor",
            "color": "yellow",
            "background": "bgYellow",
            "prefix": "feat",
            "priority": 2
        },
        {
            "type": "patch",
            "color": "green",
            "background": "bgGreen",
            "prefix": ["bugfix", "patch", "fix"],
            "priority": 1
        },
        {
            "type": "none",
            "color": "white",
            "background": "bgWhite",
            "prefix": ["docs"],
            "priority": -99999999
        }
    ]
}
